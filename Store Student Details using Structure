#include <stdio.h>

struct Student {
    int roll;
    char name[30];
    int age;
};

int main() {
    struct Student s;

    printf("Enter Roll Number: ");
    scanf("%d", &s.roll);

    printf("Enter Name: ");
    scanf("%s", s.name);

    printf("Enter Age: ");
    scanf("%d", &s.age);

    printf("\nStudent Details:\n");
    printf("Roll: %d\nName: %s\nAge: %d\n", s.roll, s.name, s.age);

    return 0;
}
