#include <stdio.h>

int main() {
    int marks[10], n, i, j, temp;

    printf("Enter number of students: ");
    scanf("%d", &n);

    printf("Enter marks:\n");
    for(i = 0; i < n; i++) {
        scanf("%d", &marks[i]);
    }

    for(i = 0; i < n; i++) {
        for(j = i + 1; j < n; j++) {
            if(marks[i] > marks[j]) {
                temp = marks[i];
                marks[i] = marks[j];
                marks[j] = temp;
            }
        }
    }

    printf("Sorted marks:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", marks[i]);
    }

    return 0;
}
