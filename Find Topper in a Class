#include <stdio.h>

int main() {
    int marks[10], n, i, max;

    printf("Enter number of students: ");
    scanf("%d", &n);

    printf("Enter marks:\n");
    for(i = 0; i < n; i++)
        scanf("%d", &marks[i]);

    max = marks[0];
    for(i = 1; i < n; i++) {
        if(marks[i] > max)
            max = marks[i];
    }

    printf("Topper marks = %d\n", max);
    return 0;
}
