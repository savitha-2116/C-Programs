#include <stdio.h>

int main() {
    int n, i;
    int marks[50];
    int total = 0;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter marks of student %d: ", i + 1);
        scanf("%d", &marks[i]);
        total = total + marks[i];   
    }
    printf("\nTotal marks of %d students = %d", n, total);

    return 0;
}
