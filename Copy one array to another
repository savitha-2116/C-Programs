#include <stdio.h>

int main() {
    int n, i;
    int a[50], b[50];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);

    for(i = 0; i < n; i++)
        b[i] = a[i];

    printf("Copied array:\n");
    for(i = 0; i < n; i++)
        printf("%d ", b[i]);

    return 0;
}
