#include <stdio.h>

struct Employee {
    int id;
    char name[20];
};

int main() {
    int n, i, search;
    struct Employee e[10];

    scanf("%d", &n);

    for(i = 0; i < n; i++)
        scanf("%d %s", &e[i].id, e[i].name);

    scanf("%d", &search);

    for(i = 0; i < n; i++) {
        if(e[i].id == search) {
            scanf("%s", e[i].name);
            printf("Updated Record: %d %s\n", e[i].id, e[i].name);
            return 0;
        }
    }

    printf("Record not found\n");
    return 0;
}
